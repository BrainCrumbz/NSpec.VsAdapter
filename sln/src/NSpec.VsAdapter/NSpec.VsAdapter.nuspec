    <iconUrl>https://cdn.rawgit.com/nspec/NSpec.VsAdapter/master/sln/NSpecVsAdapterLogo90x90.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes></releaseNotes>
    <tags>nspec test testing bdd tdd rspec mocha adapter runner</tags>
    <language>en-US</language>
    <developmentDependency>true</developmentDependency>
